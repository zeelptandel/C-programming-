#include <stdio.h>

int main() {
    int numbers[100];
    int max, min;

    printf("Enter 100 numbers:
");
    for (int i = 0; i < 100; i++) {
        scanf("%d", &numbers[i]);
    }
    max = numbers[0];
    min = numbers[0];
    for (int i = 1; i < 100; i++) {
        if (numbers[i] > max) {
            max = numbers[i];
        }
        if (numbers[i] < min) {
            min = numbers[i];
        }
    }

    printf("Largest number is: %d
", max);
    printf("Smallest number is: %d
", min);

    return 0;
}
