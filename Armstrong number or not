#include <stdio.h>
#include <math.h>

int main(void)
{
    int num, original, remainder, n = 0;
    double sum = 0.0;

    scanf("%d", &num);
    original = num;

    while (original != 0)
    {
        original /= 10;
        n++;
    }

    original = num;

    while (original != 0)
    {
        remainder = original % 10;
        sum += pow(remainder, n);
        original /= 10;
    }

    if ((int)sum == num)
        printf("%d is an Armstrong number
", num);
    else
        printf("%d is not an Armstrong number
", num);

    return 0;
}
