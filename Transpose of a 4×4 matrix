#include <stdio.h>
#define SIZE 4

int main() {
    int matrix[SIZE][SIZE], transpose[SIZE][SIZE];
    int i, j;
    
    printf("Enter elements of %dx%d matrix:\n", SIZE, SIZE);
    for(i = 0; i < SIZE; i++) {
        for(j = 0; j < SIZE; j++) {
            scanf("%d", &matrix[i][j]);
        }
    }
    
    // Finding transpose
    for(i = 0; i < SIZE; i++) {
        for(j = 0; j < SIZE; j++) {
            transpose[j][i] = matrix[i][j];
        }
    }
    
    printf("\nOriginal Matrix:\n");
    for(i = 0; i < SIZE; i++) {
        for(j = 0; j < SIZE; j++) {
            printf("%d ", matrix[i][j]);
        }
        printf("\n");
    }
    
    printf("\nTranspose Matrix:\n");
    for(i = 0; i < SIZE; i++) {
        for(j = 0; j < SIZE; j++) {
            printf("%d ", transpose[i][j]);
        }
        printf("\n");
    }
    
    return 0;
}
