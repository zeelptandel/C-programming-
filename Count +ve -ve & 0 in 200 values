#include <stdio.h>

int main(void)
{
    int arr[200];
    int positive = 0, negative = 0, zero = 0;
    int i;

    for (i = 0; i < 200; i++)
        scanf("%d", &arr[i]);

    for (i = 0; i < 200; i++)
    {
        if (arr[i] > 0)
            positive++;
        else if (arr[i] < 0)
            negative++;
        else
            zero++;
    }

    printf("%d %d %d", positive, negative, zero);
    return 0;
}
