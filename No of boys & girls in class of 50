#include <stdio.h>

int main(void)
{
    char gender;
    int boys = 0, girls = 0;

    for (int i = 0; i < 50; i++)
    {
        scanf(" %c", &gender);
        if (gender == 'M' || gender == 'm')
            boys++;
        else if (gender == 'F' || gender == 'f')
            girls++;
    }

    printf("Boys: %d
", boys);
    printf("Girls: %d
", girls);
    return 0;
}
