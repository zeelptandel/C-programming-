#include <stdio.h>

int main() {
    int sum = 0;
    for (int n = 2; n <= 500; n++) {
        int is_prime = 1;
        for (int i = 2; i <= n / 2; i++)
            if (n % i == 0)
                is_prime = 0;
        if (is_prime)
            sum += n;
    }
    printf("Sum of prime numbers between 1 and 500 is %d", sum);
    return 0;
}
