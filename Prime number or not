    #include <stdio.h>

int main() {
    int n, is_prime = 1;
    printf("Enter a number: ");
    scanf("%d", &n);
    if (n < 2)
        is_prime = 0;
    for (int i = 2; i <= n / 2; i++)
        if (n % i == 0)
            is_prime = 0;
    if (is_prime)
        printf("Prime number");
    else
        printf("Not a prime number");
    return 0;
}
