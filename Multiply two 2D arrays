#include <stdio.h>
#define ROW1 2
#define COL1 3
#define ROW2 3
#define COL2 2

int main() {
    int arr1[ROW1][COL1], arr2[ROW2][COL2], result[ROW1][COL2];
    int i, j, k;
    
    printf("Enter elements of first %dx%d matrix:\n", ROW1, COL1);
    for(i = 0; i < ROW1; i++) {
        for(j = 0; j < COL1; j++) {
            scanf("%d", &arr1[i][j]);
        }
    }
    
    printf("Enter elements of second %dx%d matrix:\n", ROW2, COL2);
    for(i = 0; i < ROW2; i++) {
        for(j = 0; j < COL2; j++) {
            scanf("%d", &arr2[i][j]);
        }
    }
    
    // Initialize result matrix with 0
    for(i = 0; i < ROW1; i++) {
        for(j = 0; j < COL2; j++) {
            result[i][j] = 0;
        }
    }
    
    // Matrix multiplication
    for(i = 0; i < ROW1; i++) {
        for(j = 0; j < COL2; j++) {
            for(k = 0; k < COL1; k++) {
                result[i][j] += arr1[i][k] * arr2[k][j];
            }
        }
    }
    
    printf("\nResultant matrix after multiplication:\n");
    for(i = 0; i < ROW1; i++) {
        for(j = 0; j < COL2; j++) {
            printf("%d ", result[i][j]);
        }
        printf("\n");
    }
    
    return 0;
}
