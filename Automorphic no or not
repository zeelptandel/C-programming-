#include <stdio.h>

int main() {
    int n, sq, temp, is_automorphic = 1;
    printf("Enter a number: ");
    scanf("%d", &n);
    sq = n * n;
    temp = n;
    while (temp > 0) {
        if (temp % 10 != sq % 10)
            is_automorphic = 0;
        temp /= 10;
        sq /= 10;
    }
    if (is_automorphic)
        printf("Automorphic number");
    else
        printf("Not an automorphic number");
    return 0;
}
