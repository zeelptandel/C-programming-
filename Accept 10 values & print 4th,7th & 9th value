#include <stdio.h>

int main() {
    int arr[10];
    int i;
    
    printf("Enter 10 values:\n");
    for(i = 0; i < 10; i++) {
        printf("Enter value %d: ", i+1);
        scanf("%d", &arr[i]);
    }
    
    printf("\n4th value: %d\n", arr[3]);
    printf("7th value: %d\n", arr[6]);
    printf("9th value: %d\n", arr[8]);
    
    return 0;
}
