#include <stdio.h>

int main() {
    int numbers[10], sum = 0;
    float mean;
    
    printf("Enter 10 numbers:
");
    for (int i = 0; i < 10; i++) {
        scanf("%d", &numbers[i]);
        sum += numbers[i];
    }
    
    mean = sum / 10.0;
    
    printf("Sum of the 10 numbers is: %d
", sum);
    printf("Mean of the 10 numbers is: %.2f
", mean);
    
    return 0;
}
